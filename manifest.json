{
  "manifest_version": 2,
  
  "name": "xpathCut",
  "description": "FPT Slovakia (I.Collak)",
  "version": "2.2",

  "browser_action": {
    "default_icon": "icon.png"	
  },
  
  "web_accessible_resources": ["Icons/*.png"],
  
   "background": {
   "persistent":false,
    "scripts": ["background.js"]
	
},
  
   
  
  "permissions": [	
	"tabs",
    "activeTab",
	"management",
    "https://ajax.googleapis.com/",
	"storage"	
  ]
  
}
